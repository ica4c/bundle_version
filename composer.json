{
  "name": "solid/bundle_version",
  "version": "2.0.0",
  "description": "Ready to use AppStore and Google Play package version resolver tools",
  "type": "library",
  "license": "LGPL-2.1-or-later",
  "authors": [
    {
      "name": "Ruslan Gataullin",
      "email": "ruslan@solid.studio"
    }
  ],
  "keywords": [
    "appstore",
    "googleplay",
    "application version",
    "bundle version"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Solid\\VersionChecker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.1.3",
    "ext-json": "*",
    "ext-dom": "*",
    "ext-libxml": "*",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "scripts": {
    "test": "phpunit  --configuration=./phpunit.xml tests"
  }
}
